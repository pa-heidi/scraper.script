{
  "name": "ai-scraper-service",
  "version": "1.0.0",
  "description": "AI-driven data extraction platform using LLMs and Playwright",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:web": "cd web && npm run build",
    "build:all": "npm run build && npm run build:web",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "generate-plan": "ts-node scripts/generate-plan.ts",
    "generate-plan:api": "ts-node scripts/generate-plan-api.ts",
    "plan:interactive": "ts-node scripts/generate-plan.ts",
    "plan:help": "ts-node scripts/generate-plan.ts --help",
    "execute-plan": "ts-node scripts/execute-plan.ts",
    "dev:server": "nodemon",
    "dev:web": "cd web && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "start:server": "node dist/server/index.js",
    "start": "NODE_ENV=production node dist/server/index.js",
    "install:web": "cd web && npm install",
    "postinstall": "npm run install:web || true"
  },
  "keywords": [
    "scraping",
    "ai",
    "llm",
    "playwright",
    "data-extraction",
    "web-ui"
  ],
  "author": "AI Scraper Team",
  "license": "MIT",
  "dependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/uuid": "^10.0.0",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsdom": "^22.1.0",
    "llamaindex": "^0.1.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.0",
    "playwright": "^1.40.0",
    "redis": "^4.6.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.22",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
